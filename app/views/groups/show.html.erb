<head>
  <title> Group Page </title>
</head>
<body>
  <h1> Profile Page for <%= @group.name %> </h1>
  <ul>
    <% if @group.posts.any? %>
      <% @group.posts.each do |post| %>
        <li><%= post.title %></li>
      <% end %>
    <% else %>
      You haven't made any posts yet
    <% end %>
</body>

<% provide(:title, 'New Group') %>
<h1>Create a New Group</h1>

<p>
<%= form_for(@group) do |f| %>
  <%= render 'shared/group_error_messages' %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.submit "Create", class: "btn btn-large btn-primary" %>
<% end %>
</p>
